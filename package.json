{
  "name": "kristory-launcher",
  "version": "1.0.3",
  "description": "Minecraft launcher for Kristory.",
  "author": "whoisrulled",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "npx next dev --turbopack -p 9002",
    "build:ui": "npx next build",
    "build-electron": "electron-builder",
    "build": "npm run build:ui && npm run build-electron",
    "start": "npx next start",
    "lint": "npx next lint",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package",
    "clean": "node -e \"const fs = require('fs'); ['.next', '.venv', 'dist', 'node_modules', 'out', 'release', 'build', 'KRISTORYBackend.spec', 'CrylauncherBackend.spec'].forEach(p => fs.existsSync(p) && fs.rmSync(p, { recursive: true, force: true }))\""
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "electron-log": "^5.1.5",
    "electron-updater": "^6.2.1",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.9.1",
    "lucide-react": "^0.475.0",
    "next": "15.3.3",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "electron": "31.7.7",
    "electron-builder": "^24.13.3",
    "genkit-cli": "^1.13.0",
    "modclean": "^3.0.0-beta.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "5.8.3"
  },
  "build": {
    "appId": "com.kristory.launcher",
    "productName": "Kristory Launcher",
    "copyright": "Copyright Â© 2024 KRISTORY",
    "publish": {
      "provider": "github",
      "owner": "rulled",
      "repo": "kristory-launcher"
    },
    "directories": {
      "output": "release/dist",
      "buildResources": "build"
    },
    "files": [
      "out/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "extraFiles": [
      {
        "from": "release/build/KRISTORYBackend",
        "to": ".",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "icon": "public/icon.ico",
      "artifactName": "KRISTORY_Full_Setup_v${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "deleteAppDataOnUninstall": true
    }
  }
}
